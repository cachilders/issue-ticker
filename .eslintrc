{
  "env": {
      "browser": false,
      "commonjs": true,
      "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
      "ecmaFeatures": {
          "experimentalObjectRestSpread": true,
          "jsx": false
      },
      "sourceType": "module"
  },
  "plugins": [

  ],
  "globals": {
    "process": true,
    "setTimeout": true
  },
  "rules": {
      "indent": [
          "error",
          2
      ],
      "linebreak-style": [
          "error",
          "unix"
      ],
      "semi": [
          "error",
          "always"
      ]
  }
}
